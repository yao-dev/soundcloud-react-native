{
  "expo": {
    "name": "soundcloud",
    "slug": "soundcloud",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.svg",
    "splash": {
      "image": "./assets/soundcloud-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FF6D26"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "splash": {
        "backgroundColor": "#FF6D26",
        "resizeMode": "cover",
        "image": "./assets/ios-splash.png"
      }
    },
    "android": {
      "splash": {
        "backgroundColor": "#FF6D26",
        "resizeMode": "cover",
        "mdpi": "./assets/android-hdpi.png",
        "hdpi": "./assets/android-mdpi.png",
        "xhdpi": "./assets/android-xhdpi.png",
        "xxhdpi": "./assets/android-xxhdpi.png",
        "xxxhdpi": "./assets/android-xxxhdpi.png"
      }
    }
  }
}